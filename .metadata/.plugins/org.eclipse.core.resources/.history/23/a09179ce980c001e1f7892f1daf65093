package matrizes;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);

		int M = input.nextInt();
		int N = input.nextInt();
		
		int [][] A = new int [M][N];		
		int [][] B = new int [M][N];
		int [][] C = new int [M][N];
		
		for (int i = 0; i < M;i++) {
			for(int j = 0; j < N;i++) {
				A[i][j] = input.nextInt();
			}
		}
		
		for (int i = 0; i < M;i++) {
			for(int j = 0; j < N;i++) {
				B[i][j] = input.nextInt();
			}
		}
		
		for (int i = 0; i < M;i++) {
			for(int j = 0; j < N;i++) {
				C [i][j] = A[i][j] + B[i][j];
			}
		}
		
		for (int i = 0; i < M;i++) {
			for(int j = 0; j < N;i++) {
				System.out.println("A SOMA Ã© "+C[i][j]);
			}
		}
		
		
		
		sc.close();
	}
}